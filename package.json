{
    "name": "@cybearl/cypack",
    "productName": "CyPack",
    "version": "2024.12.1",
    "description": "The centralized Cybearl application and API service utilities in a single well-optimized package.",
    "author": "Cybearl <[email protected]> (https://www.cybearl.com/)",
    "license": "MIT",
    "private": false,
    "keywords": [
        "cybearl",
        "cypack",
        "utilities"
    ],
    "homepage": "https://github.com/cybearl/cypack",
    "repository": "https://github.com/cybearl/cypack",
    "bugs": "https://github.com/cybearl/cypack/issues",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/@cybearl"
    },
    "main": "index.js",
    "module": "index.mjs",
    "types": "index.d.ts",
    "engines": {
        "node": ">=20"
    },
    "files": [
        "*"
    ],
    "exports": {
        ".": {
            "import": "./index.mjs",
            "require": "./index.js"
        }
    },
    "scripts": {
        "v-bump": "yarn version --patch",
        "build": "tsup",
        "watch": "tsup --watch",
        "lint": "biome lint .",
        "lint:fix": "biome lint . --apply",
        "format": "biome format .",
        "format:fix": "biome format --write .",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "dedent-js": "^1.0.1",
        "logform": "^2.7.0",
        "loglevel": "^1.9.2",
        "server-only": "^0.0.1",
        "winston": "^3.17.0"
    },
    "peerDependencies": {
        "typescript": ">=5.4.0"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/plugin-syntax-flow": "^7.26.0",
        "@babel/plugin-transform-private-property-in-object": "^7.25.9",
        "@babel/plugin-transform-react-jsx": "^7.25.9",
        "@babel/plugin-transform-unicode-property-regex": "^7.25.9",
        "@biomejs/biome": "^1.9.4",
        "@types/copyfiles": "^2.4.4",
        "@types/node": "^22.10.2",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@types/valid-url": "^1.0.7",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.1",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "postcss": "^8.4.49",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "tsconfig-paths-webpack-plugin": "^4.2.0",
        "tsup": "^8.3.5",
        "typescript": "^5.7.2",
        "webpack": "^5.97.1"
    }
}