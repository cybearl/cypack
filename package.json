{
    "name": "@cybearl/cypack",
    "productName": "CyPack",
    "version": "2025.1.5",
    "description": "The centralized Cybearl application and API service utilities in a single well-optimized package.",
    "author": "Cybearl <[email protected]> (https://www.cybearl.com/)",
    "license": "MIT",
    "private": false,
    "keywords": [
        "cybearl",
        "cypack",
        "utilities"
    ],
    "homepage": "https://github.com/cybearl/cypack",
    "repository": {
        "url": "https://github.com/cybearl/cypack"
    },
    "bugs": "https://github.com/cybearl/cypack/issues",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/@cybearl"
    },
    "main": "index.js",
    "module": "index.mjs",
    "engines": {
        "node": ">=20"
    },
    "files": [
        "*"
    ],
    "exports": {
        "./backend": {
            "types": "./backend.d.ts",
            "import": "./backend.mjs",
            "require": "./backend.js"
        },
        "./frontend": {
            "types": "./frontend.d.ts",
            "import": "./frontend.mjs",
            "require": "./frontend.js"
        },
        ".": {
            "types": "./index.d.ts",
            "import": "./index.mjs",
            "require": "./index.js"
        }
    },
    "sideEffects": false,
    "scripts": {
        "v-bump": "yarn version --patch",
        "build": "tsup",
        "watch": "tsup --watch",
        "test:sandbox": "tsx ./tests/sandbox.ts",
        "lint": "biome lint .",
        "lint:fix": "biome lint . --write",
        "format": "biome format .",
        "format:fix": "biome format --write .",
        "check-types": "tsc --noEmit"
    },
    "dependencies": {
        "colorette": "^2.0.20",
        "dateformat": "^5.0.3",
        "dedent-js": "^1.0.1",
        "logform": "^2.7.0",
        "loglevel": "^1.9.2",
        "pino": "^9.6.0",
        "pino-pretty": "^13.0.0"
    },
    "peerDependencies": {
        "@tailwindcss/postcss": ">=4.0.0",
        "autoprefixer": ">=10.4.20",
        "postcss": ">=8.5.1",
        "react": ">=18.3.1",
        "react-dom": ">=18.3.1",
        "tailwindcss": ">=4.0.0",
        "tailwindcss-animate": ">=1.0.7",
        "typescript": ">=5.4.0"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    },
    "devDependencies": {
        "@babel/core": "^7.26.7",
        "@babel/plugin-syntax-flow": "^7.26.0",
        "@babel/plugin-transform-private-property-in-object": "^7.25.9",
        "@babel/plugin-transform-react-jsx": "^7.25.9",
        "@babel/plugin-transform-unicode-property-regex": "^7.25.9",
        "@biomejs/biome": "^1.9.4",
        "@tailwindcss/postcss": "^4.0.0",
        "@types/copyfiles": "^2.4.4",
        "@types/dateformat": "^5.0.3",
        "@types/node": "^22.10.10",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@types/valid-url": "^1.0.7",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.2",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "postcss": "^8.5.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwindcss": "^4.0.0",
        "tailwindcss-animate": "^1.0.7",
        "tsconfig-paths-webpack-plugin": "^4.2.0",
        "tsup": "^8.3.6",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "webpack": "^5.97.1"
    }
}